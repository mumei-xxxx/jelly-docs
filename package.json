{
  "name": "jelly-docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "http-server public -c-1",
    "bundle:script": "npx spago -x spago_prod.dhall build && purs-backend-es bundle-app -s -t public/index.js -y -m JellyDocs.WebMain",
    "bundle:html": "node ./generate.js",
    "bundle:tailwindcss": "tailwindcss -i ./src/index.css -o ./public/index.css",
    "bundle": "run-s bundle:*",
    "watch:script": "npx spago bundle-app -t public/index.js -m JellyDocs.WebMain -w",
    "watch:html": "npx spago run -m JellyDocs.NodeMain -w",
    "watch:tailwindcss": "tailwindcss -i ./src/index.css -o ./public/index.css -w",
    "watch": "run-p watch:*",
    "examples:hello-world": "spago bundle-app -m Example.HelloWorld.Main -t ./examples/hello-world/public/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "esbuild": "^0.15.8",
    "highlight.js": "^11.6.0",
    "http-server": "^14.1.1",
    "marked": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "purescript": "^0.15.4",
    "purs-backend-es": "^1.1.1",
    "purs-tidy": "^0.9.2",
    "spago": "^0.20.9",
    "tailwindcss": "^3.1.8",
    "xhr2": "^0.2.1"
  },
  "type": "module"
}
